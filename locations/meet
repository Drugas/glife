						act 'You mumble "I would rather not..."':
							cla
							loverrelation[lover_number] -= 2
							wantRand = rand(0, 100)
							wantRand -= 4*loverscompliance[lover_number]
							if wantRand > sub:
								cla
								'<<$loverdesc[lover_number]>> tries to push you to change it, but you successfully inform him that you do not want to. This makes him rather upset.'
								''
								dynamic $go_home		
								exit
							elseif wantRand <= sub:
								cla
								'<<$loverdesc[lover_number]>> keeps up the pressure, and because of your demeaner, and you give in. He smiles happily and takes you to the salon.'
								sub += 2
								loverrelation[lover_number] += 5
								act 'Go to Salon':		
									gt 'lover_change', 'Hair'
								end
								exit
							end
						end
					end

# $phone_theme

$RESULT = 'images/system/phone/theme/'


!theme
if pcs_phonetheme = 1:
	$RESULT += 'Bimbo'
	jump 'skipsteps'
elseif pcs_phonetheme = 2:
	$RESULT += 'Custom_Aluminus'
	jump 'skipsteps'
elseif pcs_phonetheme = 3:
	$RESULT += 'Custom_Bottlecapped'
	jump 'skipsteps'
elseif pcs_phonetheme = 4:
	$RESULT += 'Custom_Galactic'
	jump 'skipsteps'
elseif pcs_phonetheme = 5:
	$RESULT += 'Custom_Vectored'
	jump 'skipsteps'
elseif pcs_phonetheme = 6:
	$RESULT += 'Custom_Veneer'
	jump 'skipsteps'
elseif pcs_phonetheme = 7:
	$RESULT += 'Custom_Voidwalker'
	jump 'skipsteps'
elseif pcs_phonetheme = 8:
	$RESULT += 'Custom_Voidwalker Red'
	jump 'skipsteps'
elseif pcs_phonetheme = 9:
	$RESULT += 'Custom_Voidwalker Toxic'
	jump 'skipsteps'
elseif pcs_phonetheme = 10:
	$RESULT += 'Gopnik'
	jump 'skipsteps'
elseif pcs_phonetheme = 11:
	$RESULT += 'Sports'
	jump 'skipsteps'
elseif pcs_phonetheme = 12:
	$RESULT += 'Succubus'
	jump 'skipsteps'
elseif pcs_phonetheme = 13:
	$RESULT += 'Woodshop'
	jump 'skipsteps'
elseif pcs_phonetheme = 0:
	$RESULT += 'Custom_Standard'
	jump 'skipsteps'		
else 
	$RESULT += 'Custom_Standard'
	jump 'skipsteps'		
end	

:skipsteps
$RESULT += '/'



--- $phone_theme ---------------------------------


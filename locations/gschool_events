if $ARGS[0] = 'sweet':
	if Suspeh >= 90:
		Sweet_talk = 10
	elseif Suspeh >= 80:
		Sweet_talk = 5
	elseif Suspeh >= 70:
		Sweet_talk = 0
	elseif Suspeh >= 60:
		Sweet_talk = -5
	elseif Suspeh >= 60:
		Sweet_talk = -10
	else
		Sweet_talk = -20
	end
	if grupTipe = 2: Sweet_talk += 10
	if grupTipe = 3: Sweet_talk += 5
	if grupTipe = 4: Sweet_talk -= 10
	Sweet_talk += (hotcat - 5) * 5
end

if $ARGS[0] = 'rand_boy_arg':
	:rnd_boy_loop
	r = rand(0,55)
	if r > 47:
		$rand_boy = 'some boy'
	elseif npcPol[r] = 1 or r = 26 or grupTipe[r] = 1 or grupTipe[r] = -1:
		jump 'rnd_boy_loop'
	end
	$rand_boy = $npcName[r]
end


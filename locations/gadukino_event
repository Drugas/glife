			if hour >= 8 and hour <= 23: PLAY 'sound/zima2.mp3',30
			if hour >= 0 and hour <= 7: PLAY 'sound/zima2.mp3',30
		else
			if hour >= 8 and hour <= 23: PLAY 'sound/ptica.mp3',30
			if hour >= 0 and hour <= 7: PLAY 'sound/ptica.mp3',30
		end
	end
end


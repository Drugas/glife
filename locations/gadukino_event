if $args[0] = 'go_home':
	if week = 7:
		if hour = 20 or hour = 21:
			family_trip_month = month
			if month >= 6 or month <= 8:
				*clr & cla
				'Mom asks you "Well, <<$nickname>>, are you coming home with us or are you staying while in the village with your grandparents?"'
				act 'Stay': gt 'gadhouse'
				act 'To go home': gs 'gadukino_event','go_home_ender'
			else
				gs 'gadukino_event','go_home_ender'
			end
		elseif hour > 21:
			family_trip_month = month
		end
	end
end

if $args[0] = 'go_home_ender':
	*clr & cla
	minut += rand(15,25)
	!if grandma_task_counter > 0:
	!	'Before leaving grandma for your help gave you <<grandma_task_counter*200>> rubles.'
	!	money += grandma_task_counter*200
	!	grandma_task_counter = 0
	!end
	gs 'stat'
	'You get into the Volga along with the rest of your family and were soon in Pavlovo.'
	act 'In my room': gt 'bedrPar'
end

if $args[0] = 'to_gadukino':
	*clr & cla
	!family_trip_month = 1
	minut += rand(15,25)
	gs 'stat'
	'You get into the Volga and quickly reached the village Gadyukino, where your grandparents have live for forty years.'
	act 'Go into the house': gt 'gadhouse'
end


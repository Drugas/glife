if VersionCheck < 504:
    gs 'npcstatic1'
    gs 'npcstatic2'
    gs 'npcstatic3'
    gs 'npcstatic4'
	gs 'gschool_lessons', 'npc_data'
	pcs_dob = birthyear * 10000

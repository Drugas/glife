	$Cstat_pic[npctemp] = ''
	Cstat_herpes[npctemp] = 0
	Cstat_syph[npctemp] = 0
	Cstat_gon[npctemp] = 0
	Cstat_chlam[npctemp] = 0
	Cstat_Hiv[npctemp] = 0
	Cstat_genwart[npctemp] = 0
	Cstat_Thrush[npctemp] = 0

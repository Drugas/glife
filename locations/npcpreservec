	Cstat_Chem[npctemp] = 0
	Cstat_Clothpref[npctemp] = 0
	$Cstat_PersType[npctemp] = ''
	! {This closes out the cleanup variables}
	gs 'npccleanc'
end
!! Preserves $npclastsaved and npclastsavedn. Giving the last B array saved.


killvar 'npctemp'
killvar 'npctemp2'
killvar '$npctemp3'


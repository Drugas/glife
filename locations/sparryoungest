# sparryoungest

argc = arrsize('ARGS')

sparry = -1

sparryidx = 0
:sparryoungestloop
if sparryidx < arrsize('sparrage'):
	if sparry < 0 or sparry < sparrage[sparryidx]: sparry = sparrage[sparryidx]
	sparryidx += 1
	jump 'sparryoungestloop'
end

--- sparryoungest ---------------------------------


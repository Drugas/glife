			cla
			*clr
			minut += 30
			milk_cow = 0
			grandmahelp += 1
			gs 'stat'
			
			'<center><img src="images/qwest/gadukino/new/milk_cow.jpg"></center>'
			'You within the hour, milking the cows and each time you get better and better.'

			act 'Finish':gt 'Gadsarai','cow'
		end
	end
	if feed_cow = 1:
		act 'To feed the cow':
			cla
			*clr
			minut += 30
			feed_cow = 0
			grandmahelp += 1
			gs 'stat'
			
			'<center><img src="images/qwest/gadukino/new/feed_cow.jpg"></center>'
			'You fed the Dawn and went about his business.'

			act 'Finish':gt 'Gadsarai','cow'
		end
	end
	if cow_field = 1 or graze_cow = 1:
		act 'To take the cow to the herd':
			cla
			*clr
			minut += 60
			cow_field = 0
			grandmahelp += 1

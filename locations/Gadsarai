			act 'Take card from the vintage deck of erotic cards':gt 'card'
		end

		if horny >= 50:
			act 'Masturbate':gt 'selfplay'
		end

	!! Serpent is not ready yet, so remarking out til then.	
	!!	if findneck = 1:
	!!		act 'You hear a scrapping noice above you.':gt 'serpent'
	!!	end

	!!	if sunWeather = 1:
	!!		if findvel = 1 and velday ! daystart:
	!!			'You decide to spend awhile riding the <a href="exec: gt ''events'', ''cycling''">bicycle</a> around.'
	!!		end
	!!	end
		
		if findvel = 1 and hour >= 7 and hour < 20:
			act 'Go cycling':
				cls
				minut += 60
				beg += RAND(0,1)
				speed += RAND(0,2)
				fat -= RAND(2,7)
				manna+=5
				sweat+=20
				findvelrand= RAND (0,100)
				! WD: Fix old nude variable
				if $clothingworntype = 'nude':
					horny += 20
					willpower += 10
					gs 'stat'
					'<center><img src="images/qwest/gadukino/bike_nude.jpg"></center>'
					'You ride naked, avoiding crowded places and rejoicing in your boldness. It''s hard to say what you feel most: fear or the desire to be seen.'
					act 'End trip':gt $loc
					exit
				end
				if PCloSkirt > 0:
					if findvelrand > 70:
						if tanga=1:
							horny += 5
							willpower += 5
							gs 'stat'
							'<center><img src="images/qwest/gadukino/bike_tanga_1.jpg"></center>'
							'During the trip several times you ride up your skirt, revealing to passers-by a view of your panties and ass'
						else
							horny += 5
							willpower += 10
							gs 'stat'
							'<center><img src="images/qwest/gadukino/bike_tanga_2.jpg"></center>'
							'During the trip several times you ride up your skirt, revealing to passers-by a view of your naked ass'
						end
						act 'End trip':gt $loc
						exit

					else
						willpower += 5
						gs 'stat'
						'<center><img src="images/qwest/gadukino/bike_2.jpg"></center>'
						'You ride on bicycle enjoying nature around you.'
						act 'End trip':gt $loc
						exit
					end
				else
					willpower += 5
					gs 'stat'
					'<center><img src="images/qwest/gadukino/bike.jpg"></center>'
					'You ride on bicycle enjoying nature around you.'
					act 'End trip':gt $loc
					exit
				end
			end
		end

		if piratHorny >= 20 and piratfrend >= 10:gt 'pirat'
	end
end


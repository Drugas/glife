if $ARGS[0] = 'boar':
	cls
	minut += 1
	gs 'stat'
	
	'<center><H4>The barn</H4></center>'
    '<center><img src="images/qwest/gadukino/new/boar.jpg"></center>'
	'Proud grandparents - Boar Nickle.'

	if feed_boar = 1:
		act 'Feed the boar':
			cla
			*clr
			minut += 30
			grandmahelp += 1
			feed_boar = 0
			gs 'stat'
			
			'<center><img src="images/qwest/gadukino/new/feed_boar.jpg"></center>'
			'You feed the Nickle and went on about their business.'

			act 'Finish':gt 'Gadsarai','boar'
		end
	end
	act 'Go':minut += 5 & gt'Gadsarai'	
end

if $ARGS[0] = 'cow':
	cls
	minut += 1
	gs 'stat'
	
	'<center><H4>The barn</H4></center>'
    '<center><img src="images/qwest/gadukino/new/cow.jpg"></center>'
	'Grandma`s favorite - cow Dawn.'
	

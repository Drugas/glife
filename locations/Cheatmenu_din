$cheatmenu = {
	gs'stat'
	clr
	pl'<center><a href="exec:gs''stat''"><b>Exit cheat menu</b></a></center>'
	nl
	pl'<a href="exec:dynamic $cheatmenu[''permanent'']"><b>Recurrent</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''looks'']"><b>Appearance</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''stats'']"><b>Attributes and skills</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''reputation'']"><b>Relationships and Reputation</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''documents'']"><b>Documents</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''time'']"><b>Manipulating time</b></a>'
	pl'<a href="exec:dynamic $cheatmenu[''display'']"><b>Status Display</b></a>'
	if StoryLine = 0:pl'<a href="exec:dynamic $cheatmenu[''magic'']"><b>Magic</b></a>'
	nl
	pl'<a href="exec:health = vital*10 + stren*5+1000 & willpower = intel*5 + will*5+1000 & manna = (intel*magik) + magik*100 + vital*10 + rikudo & vgape = 0 & agape = 0 & nippain = 0 & dynamic $cheatmenu">Full restoration of health, manna, Reason.</a>'
	pl'<a href="exec:dynamic $cheatmenu[''money'']">Change money</a>'
	pl'<a href="exec:money = 0 & dynamic $cheatmenu">Zero money</a>'
	pl'<a href="exec:dynamic $cheatmenu[''std_cure'']">Cure all stds</a>'
	pl'<a href="exec:vgape = 0 & agape = 0 & nippain = 0 & painpub = 0 & pirs_pain_ton = 0 & mesec = 0 & spanked = 0 & SLomka = 0 & Narkoman = 0 & strongnark = 0 & SNarkTimes = 0 & nark = 0 & fingal = 0 & mosol = 0 & frost = 0 & sick = 0 & dynamic $cheatmenu[''std_cure''] & dynamic $cheatmenu[''pain_killer'']">Cure all ailments.</a>'
	nl
	if cyccustom = 0:pl'Automatic period tracking - <a href="exec:dynamic $cheatmenu[''auto_period'']">OFF</a>'
	if cyccustom = 1:pl'Automatic period tracking - <a href="exec:dynamic $cheatmenu[''auto_period'']">ON</a>'
	pl'<a href="exec:dynamic $cheatmenu[''force_preg'']">Force random pregnancy</a>'
	pl'<a href="exec:dynamic $cheatmenu[''remove_preg'']">Remove pregnancy</a>'
	pl'<a href="exec:dynamic $cheatmenu[''cycle'']"><b>Set stage of menstrual cycle</b></a>'
	if Enable_nogameover = 1:
		pl' <a href="exec:Enable_nogameover = 0 & dynamic $cheatmenu">Prevent Non-Drama GAMEOVER: ON</a>'
	else
		pl' <a href="exec:Enable_nogameover = 1 & dynamic $cheatmenu">Prevent Non-Drama GAMEOVER: OFF</a>'
	end
	nl
	if sound = 1:
		pl' <a href="exec:sound = 0 & dynamic $cheatmenu">Music: OFF</a>'
	else
		pl' <a href="exec:sound = 1 & dynamic $cheatmenu">Music: ON</a>'
	end
	pl' <a href="exec:music_on = 0 & music_off = 1">Stop current Music</a>'
	nl
	pl '<a href="exec:dynamic $cheatmenu[''dynamic'']">Dynamic Input Command</a>'
	nl
   	pl'<a href="exec:gt''pavResidential''">Teleport to Pavlovsky</a>'
   	pl'<a href="exec:gt''street''">Teleport to City</a>'
   	pl'<a href="exec:gt''gadukino''">Teleport to Gadukino</a>'
   	pl'<a href="exec:gt''dachi''">Teleport to Suburban Cooperative</a>'
	pl'<b>Warning!</b> Teleporting while in event is likely to break your game!'
    pl''
	pl'<a href="exec:dynamic $cheatmenu[''vartracker'']"><b>Variable tracker</b></a>'
}


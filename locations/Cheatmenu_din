	if ashrink < 10:pl'<a href="exec:ashrink += 1 & dynamic $cheatmenu[''looks2'']">Increase the amount to shrink</a>'
	if ashrink > 0:pl'<a href="exec:ashrink -= 1 & dynamic $cheatmenu[''looks2'']">Reduce the amount to shrink</a>'
	if ashrinkdays < 10:pl'<a href="exec:ashrinkdays += 1 & dynamic $cheatmenu[''looks2'']">Increase the number of days it takes</a>'
	if ashrinkdays > 1:pl'<a href="exec:ashrinkdays -= 1 & dynamic $cheatmenu[''looks2'']">Reduce the number of days it takes</a>'
	pl''
	if salo ! fat:pl'<a href="exec:fat = salo & dynamic $cheatmenu[''looks2'']">Balance fat</a>'
	if salo > 0:pl'<a href="exec:salo -= 5 & fat = salo & dynamic $cheatmenu[''looks2'']">Decrease the fat</a>'
	if salo > 0:pl'<a href="exec:salo = 0 & fat = salo & dynamic $cheatmenu[''looks2'']">Zero fat</a>'
	pl'Fat (<<salo>>): <a href="exec:salo += 5 & fat = salo & dynamic $cheatmenu[''looks2'']">+5</a> <a href="exec:salo += 10 & fat = salo & dynamic $cheatmenu[''looks2'']">+10</a> <a href="exec:salo += 25 & fat = salo & dynamic $cheatmenu[''looks2'']">+25</a> <a href="exec:salo += 50 & fat = salo & dynamic $cheatmenu[''looks2'']">+50</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''tatoo'']">Tattoo removal</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''looks'']">Go to page 1</a>                <a href="exec:dynamic $cheatmenu[''looks3'']">Go to page 3</a>'

# Cheatmenu_din
!All the active dynamics for the cheat menu are here

$cheatmenu['work'] = {
	if cheatWork = 0:
		cheatWork = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatWork = 1:
		cheatWork = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['hair'] = {
	if cheatHapri = 0:
		cheatHapri = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatHapri = 1:
		cheatHapri = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['enema'] = {

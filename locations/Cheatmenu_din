$cheatmenu['looks3'] = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $cheatmenu"><b>Cheat menu</b></a></center>'
	pl''
	pl'<<$hair>>'
	if hcol =< 0:
		pl'Your hair is now (prev) <<$hcol>> <a href="exec:hcol +=1 & dynamic $cheatmenu[''looks3'']">(next)</a>'
	elseif hcol > 0 and hcol < 3:
		pl'Your hair is now <a href="exec:hcol -=1 & dynamic $cheatmenu[''looks3'']">(prev)</a> <<$hcol>> <a href="exec:hcol +=1 & dynamic $cheatmenu[''looks3'']">(next)</a>'
	elseif hcol => 3:
		pl'Your hair is now <a href="exec:hcol -=1 & dynamic $cheatmenu[''looks3'']">(prev)</a> <<$hcol>> (next)'
	end
	if hairlength > 575:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a>'
	elseif hairlength > 500 and hairlength =< 575:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a>'
	elseif hairlength =< 500 and hairlength => 100:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 100 and hairlength => 25:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 100 and hairlength => 25:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 25:
		pl'Hairlength (<<hairlength>>)  <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	end
	pl''
	pl'<<$glaza>>'
	if resnic < 2:pl'<a href="exec:resnic += 1 & dynamic $cheatmenu[''looks3'']">Enlarge lashes</a>'
	if resnic > 0:pl'<a href="exec:resnic -= 1 & dynamic $cheatmenu[''looks3'']">Shrink lashes</a>'
	if glaraz < 3:pl'<a href="exec:glaraz += 1 & dynamic $cheatmenu[''looks3'']">Increase the size of the eye</a>'
	if glaraz > 0:pl'<a href="exec:glaraz -= 1 & dynamic $cheatmenu[''looks3'']">Reduce the size of the eye</a>'
	if glacol <= 0:
		pl'Your eyes are now (prev) <<$glacol>> <a href="exec:glacol +=1 & dynamic $cheatmenu[''looks3'']">(next)</a>'
	elseif glacol > 0 and glacol < 3:
		pl'Your eyes are now <a href="exec:glacol -=1 & dynamic $cheatmenu[''looks3'']">(prev)</a> <<$glacol>> <a href="exec:glacol +=1 & dynamic $cheatmenu[''looks3'']">(next)</a>'
	elseif glacol >= 3:
		pl'Your eyes are now <a href="exec:glacol -=1 & dynamic $cheatmenu[''looks3'']">(prev)</a> <<$glacol>> (next)'
	end
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''looks'']">Go to page 1</a>                <a href="exec:dynamic $cheatmenu[''looks2'']">Go to page 2</a>'
}

$cheatmenu['parameters'] = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $cheatmenu"><b>Cheat menu</b></a> - <a href="exec:dynamic $cheatmenu[''looks'']"><b>Outward Appearance</b></a></center>'
	pl''
	pl'<<strenbuf>> - strenbuf'
	pl'<<vitalbuf>> - vitalbuf'
	pl'<<vmeat>> - vmeat'
	pl'<<vfat>> - vfat'
	pl'<<Kves>> - Kves'

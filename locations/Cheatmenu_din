	if cheatVib = 1:$cheatVib = '<b>YES</b>'
	if cheatpirate = 0:$cheatpirate = 'NO'
	if cheatpirate = 1:$cheatpirate = '<b>YES</b>'

	pl'<center><a href="exec:dynamic $cheatmenu"><b>Cheat menu</b></a></center>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''work'']">More than one job is possible</a>: <<$cheatWork>>'
	pl'<a href="exec:dynamic $cheatmenu[''hair'']">Always combed</a>: <<$cheatHapri>>'

	pl''
	pl'<<$vagina>>'
	if vagina > 0:pl'<a href="exec:vagina = 0 & dynamic $cheatmenu[''looks2'']">Reinstate virginity</a>'
	if vagina <= 31:pl'<a href="exec:vagina += 5 & dynamic $cheatmenu[''looks2'']">Enlarge the vagina</a>'

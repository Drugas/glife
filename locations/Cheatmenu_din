	if vshrink < 10:pl'<a href="exec:vshrink += 1 & dynamic $cheatmenu[''looks2'']">Increase the amount to shrink</a>'
	if vshrink > 0:pl'<a href="exec:vshrink -= 1 & dynamic $cheatmenu[''looks2'']">Reduce the amount to shrink</a>'

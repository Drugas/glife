$hypnoDaychange = {
	if hypnoStrength = 0 and hypnoTime > 0:
		if hypnoAddict = 0:
			hypnoStrength = 19
			hypnoTime -= 1
		elseif hypnoAddict = 1:
			if rand(0,2) = 0: 
				hypnoStrength = 19
				hypnoTime -= 1
				if hypnoTime < 7:hypnoAddict = 0
			end
		else
			if rand(0,4) = 0: 
				hypnoStrength = 19
				hypnoTime -= 1
				if hypnoTime < 10:hypnoAddict = 1 & hypnoBlock = 0
			end
		end
	elseif hypnoStrength > 0:
		hypnoStrength -= 1
	end
	if hypnoDay > 6: 
		if hypnoAddict = 1:
			hypnoWithdrawal = 1
		elseif hypnoAddict = 2:
			hypnoWithdrawal = 2
		end
	end
}

$hypnoAddiction = {
	if hypnoAddict = 1:
		if cumloc[0] > 0 or cumloc[3] > 0: 
			horny += 1
			mana += 1
			willpower += 1
		end
	else
		if cumloc[0] > 0 or cumloc[3] > 0:
			horny += 2
			mana += 2
			willpower += 2
		end
	end
}


# gdksport
!{Change log 14/03/2016
reformatted to use arguments
standardized prices and number of lessons to 300 <b>₽</b> for 30 lessons
removed dynamic call from 'Wash in shower'
reduced dance skill gain to 0-2 per lesson
capped dance skill at 75 for lessons
reduced dance skill requirement for Albina quest to 75
added alternate starts to Albona quest based on relationship level
moved content of 'volley' to 'gdksport','volley'
fixed missing ShowerIvan = 1 and ShowerIvan = 2 variables 
29/03/2016
fixed Lariska quest start to run even if the fight was more than a week ago
}

if $ARGS[0] = 'start':
	CLOSE ALL
	$metkaM = $ARGS[0]
	$locM = $CURLOC
	$metka = $ARGS[0]
	$loc = $CURLOC
    $location_type = 'public_indoors'
	gs 'stat'
	fcolor = rgb(0, 0, 0)
	bcolor = rgb(255, 255, 255)
	lcolor = rgb(106, 90, 205)
	'<center><b><font color="maroon">community center</font></b></center>'
	'<center><img src="images/qwest/alter/gdksport.jpg"></center>'
	'The community center sports club offers lessons in <a href="exec: gt ''gdksport'',''racing1''">Racing</a>, <a href="exec:gt ''gdksport'',''dance1''">Dance</a> and <a href="exec:gt ''gdksport'',''vball1''">Volleyball</a>.'

	if kotovLoveQW > 0 and hour = 16:
		'You see <a href="exec:minut += 3 & gt ''kotovEv''">Vitek Kotov</a> with a large sports bag entering the sports club.'
	elseif grupNPC[3] >= 80 and hour = 17 and ivansportday ! daystart:
		'You see <a href="exec:minut += 3 & gt ''IvanEv''">Ivan Prokhorov</a> with a large sports bag entering the sports club.'
	end
	
	! WD: Uninitialised ~ 'UNSTARTER' allways false
!	if grupNPC[5] >= 80 and fedorKozlovQW = 0 and GorSlut = 0 and UNSTARTER = 1: gt 'gdksport', 'fedor_date'
	!!event does not start as it is incomplete, needs home location and cinema location!!
	
	act 'Go outside':
		if $clothingworntype = 'nude':
			msg'<b><font color = red>You need to get dressed before leaving.</font></b>'
			gt 'gdksport', 'start'
		elseif lariskaQwestStart <= daystart and lariskaQwestStart ! 0 and lariskaHelp ! 1:
			gt 'LariskaStart', 'start'
		elseif lariskaQwestDay = daystart:
			gt 'LariskaStart', 'end'
		else
			gt 'gdk'
		end
	end

	act 'Go to the toilets':
		if $clothingworntype = 'nude':
			msg'<b><font color = red>You need to get dressed before leaving.</font></b>'
			gt 'gdksport', 'start'
		elseif lariskaQwestStart <= daystart and lariskaQwestStart ! 0 and lariskaHelp ! 1:
			gt 'LariskaStart', 'start'
		elseif lariskaQwestDay = daystart:
			gt 'LariskaStart', 'end'
		else
			gt 'gdktoilet', 'start'
		end
	end

	act 'Change clothes in the locker room':gt 'clothing_QV', 'list'

	if  $clothingworntype ! 'exercise' and (albinaQWdance > 0 or gsAboDance > 0 or gsAboBeg > 0 or gsAboVolley > 0):'You are not dressed in sports clothes.'
	if sweat > 19:
		act 'Wash in the shower':gt 'gdksport', 'shower'
	end
	if hour < 16 or hour >= 20: exit
	if gsAboDance > 0 and $clothingworntype = 'exercise' and albinaQWdance = 0: 
		act 'Take a dance lesson, you have <<gsAboDance>> lessons remaining':gt 'gdksport', 'dance2'
	end
	if albinaQWdance > 0 and AlbinaBlokDance = 0 and ZvezdEnd = 0 and $clothingworntype = 'exercise':
		act 'Train with Starlets':gt 'gdksport', 'starlets'
	end
	if gsAboBeg > 0 and $clothingworntype = 'exercise':
		act 'Take a track lesson, you have <<gsAboBeg>> lessons remaining':gt 'gdksport', 'racing2'
	end
	if gsAboVolley > 0 and volleysostav = 0 and $clothingworntype = 'exercise':
		if week ! 5:
			act 'Take a volleyball lesson, you have <<gsAboVolley>> lessons remaining':gt 'gdksport', 'vball2'
		else
			act 'Take a volleyball lesson, you have <<gsAboVolley>> lessons remaining':gt 'gdksport', 'vball3'
		end
	end
	if volleysostav > 0 and $clothingworntype = 'exercise':
		if week ! 6 and volkomday ! daystart:
			act 'Play with the team':gt 'gdksport', 'vball4'
		elseif week = 6 and sorevday ! daystart:
			act 'Play in Tournament': gt 'gdksport', 'vball5'
		end
	end
end
			
if $ARGS[0] = 'racing1':
	cls
	gs 'stat'
	'Race training cost 300 <b>₽</b> for 30 lessons.'

	if money >= 300 and gsAboBeg = 0:
		act 'Buy 30 racing lessons (300 RUB)':
			cls
			money -= 300
			gsAboBeg += 30
			gs 'stat'
			'You paid for 30 racing lessons.'
			act 'Leave':gt 'gdksport', 'start'
		end
	end

	act 'Leave':gt 'gdksport', 'start'
end


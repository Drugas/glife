$Phone_contactlist = {
	clr
	if hour < 10:
		if minut > 9:pl'<center><b>0<<hour>> : <<minut>></b></center>'
		if minut < 10:pl'<center><b>0<<hour>> : 0<<minut>></b></center>'
	elseif hour > 9:
		if minut > 9:pl'<center><b><<hour>> : <<minut>></b></center>'
		if minut < 10:pl'<center><b><<hour>> : 0<<minut>></b></center>'
	end
	pl'<center><img src="images/pic/icon/icon_statusbar.png"><br></center>'
	pl'<center><font size=6>Contacts</font></center>'
	pl'<center><img src="images/pic/icon/icon_call.png"><br></center>'
	if StoryLine = 1:pl'<center><img src="images/pic/phone/icons/icon_npc1.png">..................................................<a href="exec:dynamic $Phone_call"><b>Call</b></a></center>'
	if hour >= 8 and hour <= 19:
		if telalla > 0:
			pl'<center><img src="images/pic/phone/icons/icon_ira1.png">..................................................<a href="exec:$callerid = ''images/pic/phone/icons/icon_ira1.png'' & $telsob = ''Alla'' & gt ''telefon'', ''mobilaraz''"><b>Call</b></a></center>'
		end

		if telmasha > 0:
			pl'<center><img src="images/pic/phone/icons/icon_masha.png">..................................................<a href="exec:$callerid = ''images/pic/phone/icons/icon_masha.png'' & $telsob = ''Masha'' & gt ''telefon'', ''mobilaraz''"><b>Call</b></a></center>'
		end

		if telkat > 0:
			pl'<center><img src="images/pic/phone/icons/icon_kat.png">..................................................<a href="exec:$callerid = ''images/pic/phone/icons/icon_kat.png'' & $telsob = ''Kate'' & gt ''telefon'', ''mobilaraz''"><b>Call</b></a></center>'
		end

		if bfA > 0 and svidanieA = 0:
			pl'<center><img src="images/pic/phone/icons/icon_bf'+boypicA+'.png">..................................................<a href="exec:gt ''telefon'', ''mobilarazboy''"><b>Call</b></a></center>'
		end

	else
		if telalla > 0:
			pl'<center><img src="images/pic/phone/icons/icon_ira1.png">..................................................<a href="exec:$callerid = ''icon_ira1'' & dynamic $phone_call_reject"><b>Call</b></a></center>'
		end

		if telmasha > 0:
			pl'<center><img src="images/pic/phone/icons/icon_masha.png">..................................................<a href="exec:$callerid = ''icon_masha'' & dynamic $phone_call_reject"><b>Call</b></a></center>'
		end

		if telkat > 0:
			pl'<center><img src="images/pic/phone/icons/icon_kat.png">..................................................<a href="exec:$callerid = ''icon_kat'' & dynamic $phone_call_reject"><b>Call</b></a></center>'
		end

		if bfA > 0 and svidanieA = 0:
			pl'<center><img src="images/pic/phone/icons/icon_bf'+boypicA+'.png">..................................................<a href="exec:$callerid = ''icon_bf''+boypicA & dynamic $phone_call_reject"><b>Call</b></a></center>'
		end
	end
	if nastjaqw > 0:
		pl'<center><img src="images/pic/phone/icons/icon_nastja.png">..................................................<a href="exec:gs ''telefon'', ''start''"><b>Call</b></a></center>'
	end
	
	pl'<center><a href="exec:dynamic $Phone_menu"><img src="images/pic/icon/back.png"></a></center>'
}

$Phone_notebook = {
	clr
	pl'<center><font size=6>Planner</font></center>'
	rent = -1
	if BuyHous = 2 and ArendHouseSL > 0:rent = ArendHouseSL
	preg1 = pregtime
	d1 = day
	w1 = week
	m1 = month
	i = 0
	:pnotebook
		daynum = daystart+i
		$calendarday = ''
		if preg1 = 280:$calendarday += '<br>&emsp;<b>â€¢</b> <font color=''red''><b>Birthing day!</b></font>'
		if storyline = 1 and SchoolAtestat = 0 and kanikuli > 0:
			if m1 = 10 and d1 = 27:$calendarday += '<br>&emsp;<b>â€¢</b> First day of Autumn break.'
			if m1 = 11 and d1 = 4:$calendarday += '<br>&emsp;<b>â€¢</b> Last day of Autumn break.'
			if m1 = 12 and d1 = 29:$calendarday += '<br>&emsp;<b>â€¢</b> First day of Winter break.'
			if m1 = 1 and d1 = 13:$calendarday += '<br>&emsp;<b>â€¢</b> Last day of Winter break.'
			if m1 = 3 and d1 = 25:$calendarday += '<br>&emsp;<b>â€¢</b> First day of Spring break.'
			if m1 = 3 and d1 = 31:$calendarday += '<br>&emsp;<b>â€¢</b> Last day of Spring break.'
			if m1 = 5 and d1 = 25 and (age = 17 or (age = 16 and birthmonth <= 5 and birthday <= 25)):
				$calendarday += '<br>&emsp;<b>â€¢</b> Graduation Day!'
			elseif m1 = 6 and d1 = 1:
				$calendarday += '<br>&emsp;<b>â€¢</b> First day of Summer break.'
			end
			if m1 = 8 and d1 = 31:$calendarday += '<br>&emsp;<b>â€¢</b> Last day of Summer break.'
		end
		if w1 < 5 and (workSec >= 1 or PersSecWork = 1):$calendarday += '<br>&emsp;<b>â€¢</b> Working as a secretary. <i>(9:00 - 17:00)</i>'
		if w1 < 5 and workFabrika = 1:
			$calendarday += '<br>Working as a seamstress. '
			if age < 18:$calendarday += '<i>(16:00 - 20:00)</i>' else $calendarday += '<i>(8:00 - 16:00)</i>'
		end
		if w1 > 1 and w1 <= 5 and workhosp = 1:$calendarday += '<br>&emsp;<b>â€¢</b> Working as nurse. <i>(8:00 - 16:00)</i>'
		if worksalon = 1 and (w1 = 1 or w1 = 3 or w1 = 5):$calendarday += '<br>&emsp;<b>â€¢</b> Working as masseuse. <i>(9:00 - 17:00)</i>'
!		if maidqw > 0:$calendarday += '<br>&emsp;<b>â€¢</b> Working as maid. <i>(16:00 - 19:00)</i>'
		if pfilmday[i] > 0:$calendarday += '<br>&emsp;<b>â€¢</b> Porn shoot: <i><<$porndesc[pfilmday[i]]>></i>'
		if d1 = 25 and (PersSecWork = 1 or workKafe = 1 or workhosp = 1 or worksalon = 1 or tanwork = 1 or workSec = 1 or workFabrika = 1):$calendarday += '<br>&emsp;<b>â€¢</b> <i>Pay day.</i>'
		if d1 = 31 and m1 = 12 and (year - year_start) < 2 and StoryLine = 1:$calendarday += '<br>&emsp;<b>â€¢</b> New Year''s party at 14:00!'
		if m1 = 1 and d1 = 1:$calendarday += '<br>&emsp;<b>â€¢</b> New year!'
		if (sisboyQW-2) mod 2 = 1 and sisboyday+1 = daynum:
			if sisboytrioQW = 1:
				$calendarday += '<br>&emsp;<b>â€¢</b> <font color="orange">Threesome with Anja and Roma at 18:00</font>'
			else
				$calendarday += '<br>&emsp;<b>â€¢</b> <font color="orange">Anja having sex with Roma at 18:00</font>'
			end
		end
		if rent = 0:$calendarday += '<br>&emsp;<b>â€¢</b> Rent on apartment expires!'
		if birthday = d1 and birthmonth = m1:$calendarday += '<br>&emsp;<b>â€¢</b> My birthday!'
		if kid > 0:
			k1 = kid
			:kiddieloop
				if daykid[k1] = d1 and monthkid[k1] = m1:$calendarday += '<br>&emsp;<b>â€¢</b> Birthday of my <<$polreb[k1]>> <<$kidname[k1]>>, my <<kid>>. baby!'
			if k1 > 1:k1 -= 1 & jump 'kiddieloop'
		end
		
		if $calendarday ! '':
			p '<b><<$monthName[m1]>> <<d1>>, <<$week[w1]>>:</b>'
			pl $calendarday
		end
	if i < 14:
		preg1 += 1
		rent -= 1
		i += 1
		w1 += 1
		if w1 > 7:w1 = 1
		d1 += 1
		if monthsend[m1] < d1: m1 += 1 & d1 = 1
		jump 'pnotebook'
	end
	killvar 'calendarday'
	killvar 'rent'
	killvar 'preg1'
	killvar 'd1'
	killvar 'w1'
	killvar 'm1'
	killvar 'daynum'
	pl'<center><a href="exec:dynamic $Phone_menu"><img src="images/pic/icon/back.png"></a></center>'
}


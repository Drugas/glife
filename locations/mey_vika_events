				gs 'stat'
				gt 'pavResidential'
			end
		end
	end
end

if $args[0] = 'beach_event0':
	*clr & cla
	if args[1] < 1:
		'<center><img src="images/qwest/alter/meynolds/vika_beach1.jpg"></center>'
		func('mey_vika_strings', 'case_beach1')

		act 'To go on a wild beach': gt 'mey_vika_events', 'beach_event0', 1
	else
		*clr & cla
		mey_vika['qw_day'] = day
		'<center><img src="images/qwest/alter/meynolds/vika_beach2.jpg"></center>'
		func('mey_vika_strings', 'case_beach2')

		if hour < 20:
			! change clothes to swimsuit
			gs 'zz_clothing', 'redress', 2
			flag_tanga = tanga
			tanga = 0
			act 'Swim':
				*clr & cla
				minut += 10
				manna += 10
				dynamic $showerdin
				gs 'stat'
				'<center><img src="images/qwest/alter/meynolds/vika_beach3.jpg"></center>'
				func('mey_vika_strings', 'case_beach3')

				act '...': gt 'mey_vika_events', 'beach_event0', 1
			end
			act 'Tan':
				*clr & cla
				minut += 10
				manna += 10
				dynamic $showerdin
				gs 'stat'
				'<center><img src="images/qwest/alter/meynolds/vika_beach4.jpg"></center>'
				func('mey_vika_strings', 'case_beach4')

				act '...':
					*clr & cla
					horny += iif(horny < 60, 50, 20)
					minut += 5
					'<center><img src="images/qwest/alter/meynolds/vika_beach5.jpg"></center>'
					func('mey_vika_strings', 'case_beach5')

					! check celostnosti
					if vagina = 0:
						! if virgin go to the beach
						*clr & cla
						minut += rand(60,90)
						func('mey_vika_strings', 'case_beach16')

						gs 'mey_vika_events', 'beach_ender'
					else
						! gg not a girl, continue
						act '...':
							*clr & cla
							horny += 20
							minut += 5
							gs 'stat'
							'<center><img src="images/qwest/alter/meynolds/vika_beach6.jpg"></center>'
							func('mey_vika_strings', 'case_beach6')

							act '...':
								*clr & cla
								horny += 20
								minut += 5
								gs 'stat'
								'<center><img src="images/qwest/alter/meynolds/vika_beach7.jpg"></center>'
								func('mey_vika_strings', 'case_beach7')

								act '...':
									*clr & cla
									'<center><img src="images/qwest/alter/meynolds/vika_beach8.jpg"></center>'
									func('mey_vika_strings', 'case_beach8')

									act '...':
										*clr & cla
										horny += 20
										minut += 30
										gs 'stat'
										'<center><img src="images/qwest/alter/meynolds/vika_beach9.jpg"></center>'
										func('mey_vika_strings', 'case_beach9')

										act '...':
											*clr & cla
											'<center><img src="images/qwest/alter/meynolds/vika_beach10.jpg"></center>'
											func('mey_vika_strings', 'case_beach10')

											act '...':
												*clr & cla
												horny = 0
												minut += 10
												gs 'stat'
												'<center><img src="images/qwest/alter/meynolds/vika_beach11.jpg"></center>'
												func('mey_vika_strings', 'case_beach11')

												mey_vika['mey_vika_qw'] = 20
												gs 'mey_vika_events', 'beach_ender'
											end
										end
									end
								end
							end
						end
					end
				end
			end
		else
			gs 'mey_vika_events', 'beach_ender'
		end
	end
end


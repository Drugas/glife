						                cls
						                minut += 45
						                gs 'stat'
						                '<center><img src="images/locations/shared/train/cityplat.jpg"></center>'
						                'You go to the northern district of the City.'
						                act 'Leave':minut += 5 & nroad = 0 & gt 'Vokzal'
					                    end
						            end
								end
							end
						end
					end
				end
			end
		end
	end
end





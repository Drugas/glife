	elseif a >= 10:
		$ind = '<font color="red"><font face="Lucida Console" size=4> ██__________________ </font><<a1>></font>'
	elseif a >= 5:
		$ind = '<font color="red"><font face="Lucida Console" size=4> █___________________ </font><<a1>></font>'
	elseif a >= 1:
		$ind = '<font color="red"><font face="Lucida Console" size=4> ▓___________________ </font><<a1>></font>'
	elseif a <= 0 :
		$ind = '<font color="red"><font face="Lucida Console" size=4> ____________________ </font><<a1>></font>'
	end
end


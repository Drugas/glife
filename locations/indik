	elseif a >= 30:
		$ind = '<font color="red"><font face="Lucida Console" size=4> ██████______________ </font><<a1>></font>'
	elseif a >= 25:
		$ind = '<font color="red"><font face="Lucida Console" size=4> █████_______________ </font><<a1>></font>'

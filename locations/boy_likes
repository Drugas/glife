	figurBoyA = 0
	titBoyA = 2
	haerBoyA = 3
	
	clotTypePrefA = 4
	clotQualPrefA = 2
	clotTopPrefA = 3
	bottShorPrefA = 3
	clotThinPrefA = 3
	bimbPrefA = 1
	pierPrefA = 2
	tattPrefA = 1
	lipsPrefA = 2
	makePrefA = 3
	IQPrefA = 0
	harakBoyA = 2
	financeA = 2
	
	boybodyrand = 1
	boybodyrandA = boybodyrand
	if boybodyrandA = 1: $boybody = 'tall'
	$boybodyA = $boybody 
	
	boybodrand = 2
	boybodrandA = boybodrand
	if boybodrand = 2: $boybod = 'athletic'
	$boybodA = $boybod
	izvrat = 1
	izvratA = izvrat
	
	'figurBoyA is <<figurBoyA>>, titBoyA is <<titBoyA>>, clotTypePrefA is <<clotTypePrefA>>, clotQualPrefA is <<clotQualPrefA>>, clotTopPrefA is <<clotTopPrefA>>'
	'bottShorPrefA is <<bottShorPrefA>>, clotThinPrefA is <<clotThinPrefA>>, bimbPrefA is <<bimbPrefA>>, pierPrefA is <<pierPrefA>>, tattPrefA is <<tattPrefA>>'
	'lipsPrefA is <<lipsPrefA>>, makePrefA is <<makePrefA>>, IQPrefA is <<IQPrefA>>, compliance is <<compliance>>.'
end

